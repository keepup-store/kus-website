      <main>

            <!-- Render banners if available -->
         {{#landscape_banner}}
         <section class="breadcrumb__area include-bg mb-50 d-none d-lg-block">
               <img width="100%" src="{{ landscape }}" style="object-fit: cover;" />
         </section>
         {{/landscape_banner}}
         {{^landscape_banner}}
         <section class="breadcrumb__area include-bg mb-50 pt-150 pb-150 breadcrumb__overlay breadcrumb__style-3 d-none d-lg-block" data-background="store-assets/img/banner/banner.jpg" style="background-image: url(&quot;store-assets/img/banner/banner.jpg&quot;);">
            <div class="container">
               <div class="row">
                  <div class="col-xxl-12">
                     <div class="breadcrumb__content text-center p-relative z-index-1">
                        <h3 class="breadcrumb__title">Welcome to Our Store</h3>
                        <p class="text-white">Start your shopping journey with us – browse, add to cart, and place your order hassle-free.</p>
                     </div>
                  </div>
               </div>
            </div>
         </section>
         {{/landscape_banner}}
         
         {{#portrait_banner}}                        
         <section class="breadcrumb__area include-bg mb-50 d-block d-lg-none">
            <img width="100%" src="{{ portrait }}" style="object-fit: cover;" />
         </section>
         {{/portrait_banner}}
         {{^portrait_banner}}
         <section class="breadcrumb__area include-bg mb-50 pt-150 pb-150 breadcrumb__overlay breadcrumb__style-3 d-block d-lg-none" data-background="store-assets/img/banner/banner.jpg" style="background-image: url(&quot;store-assets/img/banner/banner.jpg&quot;);">
            <div class="container">
               <div class="row">
                  <div class="col-xxl-12">
                     <div class="breadcrumb__content text-center p-relative z-index-1">
                        <h3 class="breadcrumb__title">Welcome to Our Store</h3>
                        <p class="text-white">Start your shopping journey with us – browse, add to cart, and place your order hassle-free.</p>
                     </div>
                  </div>
               </div>
            </div>
         </section>
         {{/portrait_banner}}

         

         <!-- shop area start -->
         <section id="productList" class="tp-shop-area pb-120" hx-get="/store/products?page={{filter.page}}&search={{filter.search}}&category={{filter.category}}" hx-trigger="load" hx-indicator="#loader" hx-target="#productList">
            
         </section>

         <section id="loader" class="tp-shop-area pb-120">
            <h2 class="text-center"><i class="fa-solid fa-circle-notch fa-spin"></i></h2>
         </section>
         <!-- shop area end -->         
